---
- name: Delete Project & Infrastructure
  hosts: localhost
  connection: local
  gather_facts: false
  no_log: "{{ molecule_no_log }}"

  vars_files:
    - ~/.apigee-secure/apigee-hybrid-sensitive-attributes.yml
    - ../../attributes.yml

  collections:
    - bap_coe.gcp_management

  tasks:
    - name: Remove projects in list
      include_role:
        name: gcp_project_delete
